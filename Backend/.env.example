# Environment Variables for Backend

# Server Configuration
PORT=5000

# JWT Secret Key (Change this to a strong secret in production!)
# Generate a secure key: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-12345

# Admin Registration Code (Required to create new user accounts)
ADMIN_REGISTRATION_CODE=ADMIN2024

# Database Configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=root
DB_NAME=student_manager

# CORS Settings
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
